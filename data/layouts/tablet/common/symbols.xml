<?xml version="1.0" ?>
<external>
  <level mode="locked" name="symbols1">
    <row>
      <key align="left" name="Escape"/>
      <key name="1"/>
      <key name="2"/>
      <key name="3"/>
      <key name="4"/>
      <key name="5"/>
      <key name="6"/>
      <key name="7"/>
      <key name="8"/>
      <key name="9"/>
      <key name="0"/>
      <key align="right" name="BackSpace" repeatable="yes" width="1"/>
      <key align="right" name="Caribou_Prefs"/>
    </row>
    <row >
      <key align="left" name="Tab" width="1.5"/>
      <key name="plus"/>
      <key name="minus"/>
